{
    "name": "jfreites/luna",
    "description": "Content Managment Package for Laravel 5.X",
    "keywords": [
        "cms",
        "laravel"
    ],
    "homepage": "https://github.com/jfreites/luna",
    "license": "MIT",
    "authors": [
        {
            "name": "Jonathan Freites",
            "email": "hi@jonathanfreites.info"
        }
    ],
    "require": {
        "php" : "~5.6|~7.0",
        "illuminate/support": "5.1.*|5.2.*",
        "jenssegers/date": "^3.2",
        "cartalyst/sentinel": "2.0.*"
    },
    "require-dev": {
        "phpunit/phpunit" : "~4.0||~5.0",
        "scrutinizer/ocular": "~1.1",
        "squizlabs/php_codesniffer": "~2.3"
    },
    "autoload": {
        "psr-4": {
            "Jfreites\\Luna\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Jfreites\\Luna\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "format": "phpcbf --standard=psr2 src/"
    }
}
